{{#each this}}
<div class="photo-card">
    <div class="imgThumb">
        <a href="{{largeImageURL}}" class="linkImg">
            <img src="{{webformatURL}}" alt="{{tags}}" loading="lazy" width="400px" class="img"/>
        </a>
    </div>
  <div class="info">
    <p class="info-item">
      <b>Likes</b><br>{{likes}}
    </p>
    <p class="info-item">
      <b>Views</b><br>{{views}}
    </p>
    <p class="info-item">
      <b>Comments</b><br>{{comments}}
    </p>
    <p class="info-item">
      <b>Downloads</b><br>{{downloads}}
    </p>
  </div>
</div>
{{/each}}